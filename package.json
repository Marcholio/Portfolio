{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.0",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-cookie-consent": "^1.9.0",
    "react-dom": "^16.6.0",
    "react-scripts": "^2.1.3",
    "react-skeleton-css": "^1.1.0"
  },
  "devDependencies": {
    "ajv": "^6.5.4",
    "eslint-plugin-prettier": "^3.0.0",
    "node-sass-chokidar": "^1.3.4",
    "prettier": "^1.14.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build-css": "node-sass-chokidar ./src/styles -o ./src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles -o ./src/styles --watch",
    "test": "node_modules/.bin/eslint ./",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
