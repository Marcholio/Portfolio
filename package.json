{
  "name": "portfolio",
  "version": "3.5.0",
  "private": false,
  "repository": "https://github.com/Marcholio/Portfolio/",
  "homepage": "https://markustyrkko.com",
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-cookie-consent": "^5.1.3",
    "react-dom": "^16.13.0",
    "react-lazyload": "^3.0.0",
    "react-scripts": "^3.4.3",
    "react-skeleton-css": "^1.1.0"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.0.0",
    "gh-pages": "^3.1.0",
    "node-sass-chokidar": "^1.5.0",
    "prettier": "^2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build-css": "node-sass-chokidar ./src/styles -o ./src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles -o ./src/styles --watch",
    "test": "node_modules/.bin/eslint ./",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build && cp CNAME build/CNAME"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
