{
  "name": "portfolio",
  "version": "3.3.1",
  "private": false,
  "homepage": "https://markustyrkko.com",
  "dependencies": {
    "gh-pages": "^2.2.0",
    "react": "^16.13.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-cookie-consent": "^3.0.0",
    "react-dom": "^16.13.0",
    "react-lazyload": "^2.6.5",
    "react-scripts": "3.4.0",
    "react-skeleton-css": "^1.1.0"
  },
  "devDependencies": {
    "ajv": "^6.5.4",
    "eslint-plugin-prettier": "^3.0.0",
    "node-sass-chokidar": "^1.3.4",
    "prettier": "^1.14.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build-css": "node-sass-chokidar ./src/styles -o ./src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles -o ./src/styles --watch",
    "test": "node_modules/.bin/eslint ./",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build && cp CNAME build/CNAME"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
