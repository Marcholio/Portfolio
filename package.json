{
  "name": "portfolio",
  "version": "3.9.1",
  "private": false,
  "repository": "https://github.com/Marcholio/Portfolio/",
  "homepage": "https://markustyrkko.fi",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-cookie": "^4.0.3",
    "react-cookie-consent": "^6.2.2",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-lazyload": "^3.1.0",
    "react-scripts": "^5.0.0",
    "react-skeleton-css": "^1.1.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-lazyload": "^3.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gh-pages": "^3.1.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build-css": "node-sass ./src/styles -o ./src/styles",
    "watch-css": "npm run build-css && node-sass ./src/styles -o ./src/styles --watch --recursive",
    "test": "node_modules/.bin/eslint ./",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build && cp CNAME build/CNAME"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
