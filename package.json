{
  "name": "portfolio",
  "version": "3.9.1",
  "repository": "https://github.com/Marcholio/Portfolio/",
  "homepage": "https://markustyrkko.fi",
  "license": "UNLICENSED",
  "dependencies": {
    "react": "18.2.0",
    "react-cookie": "4.1.1",
    "react-cookie-consent": "8.0.1",
    "react-dom": "18.2.0",
    "react-ga": "3.3.1",
    "react-lazyload": "3.2.0",
    "react-scripts": "5.0.1",
    "react-scroll": "1.8.9",
    "react-skeleton-css": "1.1.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-lazyload": "3.2.0",
    "@types/react-scroll": "1.8.6",
    "eslint-plugin-prettier": "4.2.1",
    "gh-pages": "5.0.0",
    "node-sass": "8.0.0",
    "prettier": "2.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build-css && GENERATE_SOURCEMAP=false react-scripts build && cp CNAME build/CNAME",
    "build-css": "node-sass ./src/styles -o ./src/styles",
    "watch-css": "yarn build-css && node-sass ./src/styles -o ./src/styles --watch --recursive",
    "test": "node_modules/.bin/eslint ./",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy": "yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.4.1"
}
